{% raw %}
\newcommand{\resumeItem}[1]{\item\footnotesize{{#1 \vspace{-4pt}}}}
\newcommand{\resumeSubheading}[4]{
  \vspace{-2pt}\item
    \begin{tabular*}{1.0\textwidth}[t]{l@{\extracolsep{\fill}}r}
      \textbf{#1} & \textbf{\small #2} \\
      \textit{\small#3} & \textit{\small #4} \\
    \end{tabular*}\vspace{-6pt}
}
\newcommand{\resumeProjectHeading}[2]{
    \item \begin{tabular*}{1.0\textwidth}{l@{\extracolsep{\fill}}r}
        \small#1 & \textbf{\small #2}
    \end{tabular*}\vspace{-6pt}
}
\newcommand{\resumeSubHeadingListStart}{\begin{itemize}[leftmargin=0.0in, label={}]\setlength\itemsep{3pt}}
\newcommand{\resumeSubHeadingListEnd}{\end{itemize}}
\newcommand{\resumeItemListStart}{\begin{itemize}[leftmargin=1em]}
\newcommand{\resumeItemListEnd}{\end{itemize}\vspace{-5pt}}
{% endraw %}

\begin{document}

\begin{center}
    {\Huge \scshape {{ name | default('') }}} \\ \vspace{2pt}
    \small
    \faPhone\ {{ phone | default('') }} ~ 
    \href{mailto:{{ email | default('') }}}{\faEnvelope\ {{ email | default('') }}} ~ 
    \href{ {{ linkedin | default('#') }} }{\faLinkedin\ {{ linkedin | default('') }}} ~
    \href{ {{ github | default('#') }} }{\faGithub\ {{ github | default('') }}}
\end{center}

\section{Education}
\resumeSubHeadingListStart
{% for edu in education %}
  \resumeSubheading
    {{ "{" + edu.institution + "}" }}{ {{ edu.period }} }
    {{ "{" + edu.degree + ", {\\textbf{CGPA: " + (edu.cgpa | string) + "}}}" }}{ {{ edu.location }} }
{% endfor %}
\resumeSubHeadingListEnd

\section{Relevant Coursework}
\begin{multicols}{4}
  \begin{itemize}[itemsep=-5pt, parsep=3pt]
    {% for c in coursework %}
    \item\small {{ c }}
    {% endfor %}
  \end{itemize}
\end{multicols}
\vspace*{2.0\multicolsep}

\section{Technical Skills}
\small
\textbf{Languages:} {{ languages | join(", ") }} \\
\textbf{Tools:} {{ tools | join(", ") }} \\


\section{Projects}
\resumeSubHeadingListStart

{% for p in projects %}
  \resumeProjectHeading
    {\textbf{ {{ p.title }} } $|$ {{ p.technologies | join(", ") }} }
    { {{ p.date }} }
  \resumeItemListStart
    {% if p.summary %}
      \resumeItem{ {{ p.summary }} }
    {% endif %}
    {% for feat in p.features %}
      \resumeItem{ {{ feat }} }
    {% endfor %}
  \resumeItemListEnd
{% endfor %}


\resumeSubHeadingListEnd

\section{Experience}
\resumeSubHeadingListStart
{% for e in experience %}
  \resumeSubheading
    {{ "{" + e.company + "}" }}
    { {{ (e.start ~ ' -- ' ~ e.end) }} }
    {{ "{" + e.role + "}" }}
    { {{ ((e.city or "") ~ (", " if e.city and e.country else "") ~ (e.country or "")) }} }
  \resumeItemListStart
    {% for it in e["items"] %}
    \resumeItem{ {{ it }} }
    {% endfor %}
  \resumeItemListEnd
{% endfor %}
\resumeSubHeadingListEnd


\section{Achievements And Leadership}
\resumeSubHeadingListStart
{% for a in achievements %}
  \resumeSubheading{ {{ a.title }} }{ {{ a.link | default('') }} }{ {{ a.category | default('') }} }{}
  \resumeItemListStart
    {% for it in a["items"] %}
    \resumeItem{ {{ it }} }
    {% endfor %}
  \resumeItemListEnd
{% endfor %}
\resumeSubHeadingListEnd


\end{document}
